#include <iostream>

using namespace std;

int main()
{
    int  n, m, x = 0, y = 0, r = 0;
    cin >> n;
    cin >> m;

    int a[10][10];
    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            cin >> a[i][j];
        }

        int min = a[0][0];
        int max = a[0][0];
        for (int i = 0; i < m; i++)
        {
            for (int j = 0; j < n; j++)
            {
                if (j == n)
                {
                    x = a[i][y];
                    a[i][y] = a[i][r];
                    a[i][r] = x;
                }
                if (min > a[i][j]) {
                    min = a[i][j];
                    y = j;
                }
                if (max < a[i][j]) {
                    max = a[i][j];
                    r = j;
                }
            }
        }
        cout << "\n\n";


        for (int i = 0; i < m; i++)
        {
            cout << "\n\n";
            for (int j = 0; j < n; j++)
            {
                cout << a[i][j];
            }
        }
        cout << "\n\n";
    }
    return 0;
}
