#include <iostream>

using namespace std;
int main() {
    int n, m;
    cin >> n;
    cin >> m;
    int a[10][10];

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++)
            cin >> a[i][j];
    }

    int max = 0, min = 0;

    int mini = 0, maxi = 0;

    for (int j = 0; j < m; j++) {
        int sum = 0;
        for (int i = 0; i < n; i++)
            sum += a[i][j];
        max = sum > max ? sum : max;
        maxi = max == sum ? j : maxi;

        min = sum < min ? sum : min;
        mini = min == sum ? j :mini;
    }



    for (int i = 0; i < n; i++) {
        int temp = a[i][mini];
        a[i][mini] = a[i][maxi];
        a[i][maxi] = temp;
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++)
            cout << a[i][j];
        cout << "\n";
    }
    return 0;
}
